			<Button name="PCDC_Button">
				<Size>
					<AbsDimension x="32" y="40"/>
				</Size>
				<Anchors>
					<Anchor point="CENTER" relativeTo="PCDC_Frame" relativePoint="CENTER">
						<Offset>
							<AbsDimension x="0" y="8"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						this:RegisterForDrag("LeftButton")
						this:RegisterForClicks("LeftButtonUp", "RightButtonUp")
					</OnLoad>
					<OnDragStart>
						PCDC_OnDragStart()
					</OnDragStart>
					<OnDragStop>
						PCDC_OnDragStop()
					</OnDragStop>
					<OnClick>
						PCDC_Click()
					</OnClick>
					<OnEnter>
                		GameTooltip_SetDefaultAnchor(GameTooltip, this)
                		GameTooltip:AddLine("Player cooldowns")
						GameTooltip:AddLine("Left-click/drag to position", .8, .8, .8)
						GameTooltip:AddLine("Right-click to lock", .8, .8, .8)
                		GameTooltip:Show()
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide()
					</OnLeave>
				</Scripts>
			</Button>
